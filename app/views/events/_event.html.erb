<div id="<%= dom_id event %>">
  <h2>
    <%= event.location %>
  </h2>

  <p>
    <strong>Event Organizer:</strong>
    <%= event.creator.email %>
  </p>
  <p>
    <strong>Date:</strong>
    <%= event.date.strftime("%m/%d/%Y") %>
  </p>
  <p> <%= link_to "View Event", event_path(event) %> </p>
</div>