<h1><%= @user.email %>'s Created Events:</h1>

<% @user.created_events.each do |event| %>
  <%= render event %> <br>
<% end %>
<br>
<% if user_signed_in? %>
  <div>Currently signed in as <%= current_user.email %> </div>
  <div> <%= link_to "Sign Out", destroy_user_session_url %> </div>
<% else %>
  <div> <%= link_to "Sign In", new_user_session_path %> | 
  <%= link_to "Sign Up", new_user_registration_path %></div>
<% end %>